{
    "plugins": ["@trivago/prettier-plugin-sort-imports"],
    "importOrder": ["^react", "^@/", "^[./]"],
    "importOrderSeparation": false,
    "importOrderSortSpecifiers": true,
    "importOrderCaseInsensitive": true,
    "semi": true,
    "singleQuote": false,
    "tabWidth": 4,
    "trailingComma": "es5",
    "printWidth": 80,
    "jsxSingleQuote": false,
    "bracketSameLine": false,
    "arrowParens": "always",
    "endOfLine": "lf",
    "bracketSpacing": true
}
